<h1>Jobs</h1>

<table class"table table-striped table-bordered">
  <thead>
    <tr>
      <th>Queue</th>
      <th>Method</th>
      <th>Arguments</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @queue_classic_jobs.each do |queue_classic_job| %>
    <tr>
      <td><%= queue_classic_job.q_name  %></td>
      <td><%= queue_classic_job.method  %></td>
      <td><%= queue_classic_job.arguments.inspect%></td>
      <td><%= link_to 'Destroy', queue_classic_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br />

